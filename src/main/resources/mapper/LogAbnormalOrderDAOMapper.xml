<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.oscar.dao.InterviewDAO">


    <select id="list" resultType="com.oscar.web.controller.out.ListOutModel">
        select *
        from db_oscar_practice.interview_test
        <where>
            id in
            <foreach collection="inModel.ids" item="id" open="(" close=")" separator=",">
                #{id}
            </foreach>
        </where>
        order by id desc
    </select>
</mapper>